pipeline {
    agent any

    stages {
       stage('clean') {
            steps {
              dir('ViQuJob') {
                bat 'mvn clean compile package'
              }
            }
        }
        stage('compile') {
            steps {
              dir('ViQuJob') {
                bat 'mvn compile'
              }
            }
        }
        stage('package') {
            steps {
              dir('ViQuJob') {
                bat 'mvn package -DskipTests'
              }
            }
        }
        stage('test') {
            steps {
              dir('ViQuJob') {
                bat 'mvn clean compile test'
              }
            }
        }
    }
}
